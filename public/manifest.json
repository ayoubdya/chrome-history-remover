{
  "manifest_version": 3,
  "description": "Deletes specific URLs in history automatically",
  "name": "History Remover",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "history",
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html"
  }
}